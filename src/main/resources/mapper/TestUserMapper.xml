<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hk.dao.TestUserDao">

    <select id="getUser" resultType="com.hk.entity.TestUser">
        SELECT *
        FROM testuser
    </select>

    <insert id="addUser" parameterType="com.hk.entity.TestUser">
        INSERT INTO testuser
        VALUES (#{userName},#{password})
    </insert>
</mapper>